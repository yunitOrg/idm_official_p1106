{
  "classId": "idm.componet.official.iofficialpart",
  "comName": "公文文号主办部门",
  "className": "IofficialPart",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
          "type": "input",
          "layoutType": "inline",
          "text": "唯一标识",
          "bindKey": "ctrlId",
          "disabled": true,
          "default": "@[packageid]",
          "desc": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "固定高度",
            "bindKey": "tableHeightFlag",
            "default": false
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "表格宽度",
            "desc": "表格最大宽度",
            "bindKey": "tableMaxWidth",
            "default": "calc(100% - 100px)"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "表格高度",
            "desc": "表格最大高度",
            "bindKey": "tableMaxHeight",
            "default": "calc(100%)"
          }
        ]
      },
      {
        "type": "group",
        "text": "外层样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
            {
              "type": "inlineGroup",
              "text": "宽高",
              "children": [
                {
                  "type": "input",
                  "layoutType": "block",
                  "text": "宽",
                  "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                  "bindKey": "width",
                  "width": "60px",
                  "default": "100%"
                },
                {
                  "type": "input",
                  "layoutType": "block",
                  "text": "高",
                  "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                  "bindKey": "height",
                  "width": "60px",
                  "default": "auto"
                }
              ]
            },
            {
              "type": "box",
              "layoutType": "block",
              "text": "内外边距",
              "bindKey": "ulbox"
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "背景色",
              "bindKey": "bgColor",
              "placeholder": "请选择颜色",
              "default": {
                  "hex": "#ffffff",
                  "hex8": "#ffffff"
              }
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "容器阴影",
              "bindKey": "boxShadow",
              "default": ""
            },
            {
              "type": "border",
              "layoutType": "block",
              "text": "边框",
              "bindKey": "boxborder"
            }
        ]
      }
  ]
}